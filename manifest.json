{
"manifest_version": 3,
"name": "FUCK SHORTS",
"description": "Расширение скрывает youtube shorts",
"version": "1.0",
"icons": {
"128": "128.png"
},
"action":{
    "default_popup":"popup.html"
},
"content_scripts" : [
    {
        "matches": ["*://*.www.youtube.com/?app*"],
        "js": ["script.js"],
        "run_at": "document_idle",
        "all_frames": true
    }
],
"permissions": [],
"background": {}
}